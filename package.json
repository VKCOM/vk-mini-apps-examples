{
  "name": "mini-app",
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env PORT=30708 react-scripts start ",
    "build": "react-scripts build",
    "build:sw": "concurrently \"nodemon --exec \\\"npm run build\\\" --watch src --ext ts,tsx\" \"serve -s build -l 10888\"",
    "predeploy": "react-scripts build",
    "deploy": "vk-miniapps-deploy",
    "sw": "tsc -p tsconfig.worker.json && esbuild --bundle --tsconfig=tsconfig.worker.json --outfile=public/service-worker.js src/serviceWorker/service-worker.ts --watch"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@vkontakte/icons": "2.28.0",
    "@vkontakte/vk-bridge": "latest",
    "@vkontakte/vk-mini-apps-router": "^1.0.8",
    "@vkontakte/vkui": "5.3.1",
    "@vkontakte/vkui-tokens": "^4.35.1",
    "babel-eslint": "^10.1.0",
    "classnames": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/workbox-precaching": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vkontakte/vk-miniapps-deploy": "^0.1.5",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "react-hot-loader": "^4.13.0",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.0",
    "typescript": "^5.0.4",
    "workbox-precaching": "^7.0.0"
  },
  "resolutions": {
    "react-scripts/webpack-dev-server/yargs/yargs-parser": ">=18.1.2"
  },
  "overrides": {
    "es5-ext@^0.10.50": "0.10.53",
    "typescript": "^5.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
